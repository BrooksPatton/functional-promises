{
  "sourceMaps": "inline",
  "retainLines": true,
  "env": {
    "production": {
      "presets": ["minify"]
    }
  },
  "presets": [
    [
      "env",
      {
        "debug": true,
        "modules": false,
        "useBuiltIns": true,
        "targets": {
          "chrome": "68"
        }
      }
    ]
  ],
  "plugins": [
    ["transform-es2015-computed-properties", {
      "loose": true
    }]
  ]
  /* "plugins": ["transform-es2015-modules-commonjs", "babel-plugin-transform-runtime"] */
}
